{
  "name": "track-filter",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-materialize": "^2.3.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "react-toolbox": "^2.0.0-beta.12",
    "react-toolbox-themr": "^1.0.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "soundcloud": "^3.3.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "node src/server/index.js",
    "heroku-postbuild": "yarn && yarn run build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "toolbox": "react-toolbox-themr"
  },
  "reactToolbox": {
    "include": [
      "APP_BAR",
      "AUTOCOMPLETE",
      "BUTTON",
      "CARD",
      "CHECKBOX",
      "CHIP",
      "DIALOG",
      "DRAWER",
      "DROPDOWN",
      "INPUT",
      "LAYOUT",
      "LINK",
      "LIST",
      "MENU",
      "NAVIGATION",
      "PROGRESS_BAR",
      "RIPPLE",
      "SLIDER",
      "TOOLTIP"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "appbar-color": "var(--color-slate-grey)",
      "appbar-contrast": "var(--color-light-grey)",
      "color-blue": "#00A3DF",
      "color-green": "#97C93D",
      "color-red": "#ED3F60",
      "color-yellow": "#F2BB1A",
      "color-dark-blue": "#4A539F",
      "color-dark-green": "#1B8C5C",
      "color-dark-red": "#981C4D",
      "color-orange": "#E47226",
      "color-slate-grey": "#252531",
      "color-light-grey": "#E2E3E4",
      "color-divider": "var(--color-light-grey)",
      "color-text": "var(--color-slate-grey)",
      "color-primary": "var(--color-blue)",
      "color-primary-dark": "var(--color-dark-blue)",
      "color-accent": "var(--color-red)",
      "color-accent-dark": "var(--color-dark-red)",
      "input-text-error-color": "var(--color-red)"
    },
    "output": "src/assets/react-toolbox"
  }
}
